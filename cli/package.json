{
  "name": "libredeck-cli",
  "version": "0.1.0",
  "description": "LibreDeck CLI - Command line tools for LibreDeck management",
  "type": "module",
  "bin": {
    "sdctl": "src/cli.ts"
  },
  "scripts": {
    "dev": "bun run --watch src/cli.ts",
    "build": "bun build --compile src/cli.ts --outfile sdctl",
    "start": "bun run src/cli.ts"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/inquirer": "^9.0.7",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "streamdeck",
    "automation",
    "cli"
  ],
  "author": "",
  "license": "MIT"
}